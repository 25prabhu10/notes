{
  "name": "notes",
  "version": "1.1.0",
  "description": "Anthology of Snippets",
  "scripts": {
    "fetch": "sh utils/bash/sync.sh",
    "dev": "npm run fetch && npm run lint:fix && vuepress dev docs",
    "docs:dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "deploy": "npm run fetch && npm run lint:fix && npm run build && node deploy.js",
    "lint": "textlint ./docs/**/*.md && prettier --check ./docs/**/*.md",
    "lint:fix": "prettier --write ./docs/**/*.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/25prabhu10/notes.git"
  },
  "keywords": [
    "notes",
    "documents"
  ],
  "author": "Prabhu K Hiremath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/25prabhu10/notes/issues"
  },
  "homepage": "https://github.com/25prabhu10/notes#readme",
  "devDependencies": {
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^1.0.8",
    "@vuepress/plugin-pwa": "^2.0.0-beta.50",
    "@vuepress/plugin-register-components": "^2.0.0-beta.50",
    "@vuepress/plugin-search": "^2.0.0-beta.50",
    "dictionary-en": "^3.2.0",
    "prettier": "^2.7.1",
    "textlint": "^12.2.1",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-diacritics": "^1.0.0",
    "textlint-rule-en-capitalization": "^2.0.3",
    "textlint-rule-spelling": "^0.3.0",
    "textlint-rule-terminology": "^3.0.3",
    "textlint-rule-write-good": "^2.0.0",
    "vuepress": "^2.0.0-beta.50"
  }
}
